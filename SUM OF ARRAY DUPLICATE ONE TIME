import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
      
   Scanner scanner = new Scanner(System.in);
        int n = scanner.nextInt();
        int[] notes = new int[n];
        for (int i = 0; i < n; i++) 
        {
            notes[i] = scanner.nextInt();
        }  
        Arrays.sort(notes);
        int uniqueCount = 1; 
        for (int i = 1; i < n; i++) 
        {
            if (notes[i] != notes[i - 1]) 
            {
                uniqueCount++;
            }
        }
     System.out.println(uniqueCount);
    }
}

    
